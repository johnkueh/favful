<%= simple_form_for(@item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :name %>
    <%= f.input :summary %>
    <%= f.input :is_public %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :fsq_venue_id %>
    <%= f.input :fsq_venue_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
