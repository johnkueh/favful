<h1>Listing items</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Summary</th>
      <th>Is public</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Fsq venue</th>
      <th>Fsq venue name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.user_id %></td>
        <td><%= item.name %></td>
        <td><%= item.summary %></td>
        <td><%= item.is_public %></td>
        <td><%= item.latitude %></td>
        <td><%= item.longitude %></td>
        <td><%= item.fsq_venue_id %></td>
        <td><%= item.fsq_venue_name %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_item_path %>
